{
  "name": "aragon-website",
  "version": "1.0.0",
  "description": "The website of Aragon",
  "author": "Pierre Bertet <hello@pierre.world>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
